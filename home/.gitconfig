[user]
	name = Lumynous
	email = lumynou5.tw@gmail.com
	signingKey = ~/.ssh/lumynou5.pub
[core]
	excludesFile = ~/.gitignore
	editor = nvim
[gpg]
	format = ssh
[commit]
	gpgSign = true
[tag]
	forceSignAnnotated = true
	gpgSign = true
[init]
	defaultBranch = main
[merge]
	ff = false
[pull]
	ff = true
[alias]
	graph = log --all --graph --color --decorate --oneline
	ff = merge --ff-only
